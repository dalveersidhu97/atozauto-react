{
  "name": "AZAuto2",
  "version": "1",
  "manifest_version": 3,
  "permissions": [
    "scripting",
    "activeTab",
    "storage"
  ],
  "action": {
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "js": [
        "assets/content-script-loader.userInfo.content.ts.e50b1b16.a04cd808.js"
      ],
      "matches": [
        "https://atoz.amazon.work/*"
      ],
      "run_at": "document_idle",
      "css": [
        "info-box.css"
      ]
    },
    {
      "js": [
        "assets/content-script-loader.vto.content.ts.b6ed0629.3287663d.js"
      ],
      "matches": [
        "https://atoz.amazon.work/voluntary_time_off/*"
      ],
      "run_at": "document_idle"
    },
    {
      "js": [
        "assets/content-script-loader.vet.content.ts.2846e45e.0b54a31a.js"
      ],
      "matches": [
        "https://atoz.amazon.work/shifts/schedule/find*"
      ],
      "run_at": "document_idle"
    }
  ],
  "icons": {
    "256": "public/icons/icon_1_1.png"
  },
  "web_accessible_resources": [
    {
      "matches": [
        "https://atoz.amazon.work/*"
      ],
      "resources": [
        "assets/content.utils.cdfe0a71.js",
        "assets/comparison.utils.49eb01ae.js",
        "assets/userInfo.content.ts.e50b1b16.js",
        "assets/formatters.c3b942d2.js",
        "assets/init.content.bbae9844.js",
        "assets/vto.content.ts.b6ed0629.js",
        "assets/vet.content.ts.2846e45e.js"
      ],
      "use_dynamic_url": true
    }
  ]
}